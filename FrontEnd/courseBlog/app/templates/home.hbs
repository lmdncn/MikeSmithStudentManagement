{{outlet}}

<br><br>
<form class="ui form">

    <div class="ui stackable two column grid container">
        <div class="column"> 
            <div class="ui stackable three column grid">
                <div class="column "> <label> Student Number: </label> </div>
                <div class="six wide column "> <input type="text" name="student-num" placeholder="Student Number"> </div>
            </div>
            <div class="ui stackable three column grid">
                <div class="column "> <label> First Name: </label> </div>
                <div class="eight wide column "> <input type="text" name="first-name" placeholder="First Name"> </div>
            </div>
            <div class="ui stackable three column grid">
                <div class="column "> <label> Last Name: </label> </div>
                <div class="eight wide column "> <input type="text" name="last-name" placeholder="Last Name"> </div>
            </div>
            <div class="ui stackable three column grid">
                <div class="column "> <label> Date of Birth: </label> </div>
                <div class="six wide column "> <input type="text" name="dob" placeholder="Date of Birth"> </div>
            </div>
            <div class="ui stackable three column grid">
                <div class="column "> <label> Residency: </label> </div>
                <div class="ten wide column "> <input type="text" name="residency" placeholder="Residency"> </div>
            </div>
            <div class="ui stackable three column grid">
                <div class="column "> <label> Gender: </label> </div>
                <div class="six wide column"> <input type="text" name="gender" placeholder="Gender"> </div>
            </div>
            <br> <br>
            <div class="ui stackable column"> 
                <div class="sixteen wide column"> <button class="ui button" type="submit"> Submit </button> </div>
            </div>
        </div>
        <div class="column">
            <!--<div class="dummy-row"> </div> -->
            <div class="ui stackable centered two column grid">
                <div class="column"> </div>
            </div>
            <div class="ui stackable centered two column grid">
                <div class="column"> </div>
            </div>
            <div class="ui stackable three column grid">
                <div class="right floated ten wide column" style="border: 1px solid black;">
                    <img src="/assets/images/femalestudent.png">
                </div>
            </div>
        </div>
    </div>

</form>
